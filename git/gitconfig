[user]
	name = Kezhu Wang
	email = kezhuw@gmail.com
[push]
	default = simple
[alias]
	unstage = reset HEAD
	last = log -1 HEAD
	contributors = shortlog --summary --numbered --email
	visual = !gitk
[core]
	excludesFile = ~/.gitignore
	editor = /usr/local/bin/nvim
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[pull]
	rebase = true
